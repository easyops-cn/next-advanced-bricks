{
  "name": "@next-bricks/advanced",
  "version": "0.44.8",
  "homepage": "https://github.com/easyops-cn/next-advanced-bricks/tree/master/bricks/advanced",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-advanced-bricks.git",
    "directory": "bricks/advanced"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types",
    "docs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./dist/manifest.json": "./dist/manifest.json",
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "build": "npm run build:main && npm run build:types",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json",
    "build:manifest": "cross-env NODE_ENV=production build-next-bricks --manifest-only",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.1",
    "@ant-design/icons": "^5.5.2",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@next-core/element": "^1.2.17",
    "@next-core/http": "^1.2.12",
    "@next-core/react-element": "^1.0.36",
    "@next-core/react-runtime": "^1.7.13",
    "@next-core/runtime": "^1.60.0",
    "@next-core/theme": "^1.5.4",
    "@next-core/types": "^1.14.0",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/page-navigation": "^3.12.0",
    "antd": "^5.23.2",
    "classnames": "^2.3.2",
    "html2canvas": "^1.4.1",
    "immutability-helper": "^3.1.1",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "pdfjs-dist": "3.4.120",
    "react": "0.0.0-experimental-ee8509801-20230117",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "0.0.0-experimental-ee8509801-20230117",
    "react-grid-layout": "^1.4.3",
    "react-i18next": "^12.3.1",
    "react-sizeme": "^3.0.2"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.23.12",
    "@next-core/test-next": "^1.1.9",
    "copy-webpack-plugin": "^12.0.2"
  },
  "peerDependencies": {
    "@next-bricks/basic": "*",
    "@next-bricks/form": "*",
    "@next-bricks/icons": "*",
    "@next-bricks/markdown": "*"
  }
}
