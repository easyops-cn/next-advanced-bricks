    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@next-core/element": "^1.0.4",
    "@next-core/i18n": "^1.0.11",
    "@next-core/react-element": "^1.0.5",
    "@next-core/types": "^1.1.2",
    "@next-core/utils": "^1.3.0",
    "classnames": "^2.3.2",
    "react": "0.0.0-experimental-ee8509801-20230117"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.4.2",
    "@next-core/test-next": "^1.0.4"
  }
}
