{
  "name": "@next-bricks/markdown",
  "version": "0.0.1",
  "homepage": "https://github.com/easyops-cn/next-bricks/tree/master/bricks/markdown",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-bricks.git"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "build": "npm run build:main",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@next-bricks/icons": "^0.1.6",
    "@next-core/element": "^1.0.5",
    "@next-core/http": "^1.0.5",
    "@next-core/react-element": "^1.0.6",
    "@next-core/react-runtime": "^1.0.34",
    "@next-core/theme": "^1.1.1",
    "@next-core/types": "^1.2.0",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react": "0.0.0-experimental-ee8509801-20230117",
    "@milkdown/core": "7.2.0",
    "@milkdown/ctx": "7.2.0",
    "@milkdown/preset-commonmark": "7.2.0",
    "@milkdown/prose": "7.2.0",
    "@milkdown/react": "7.2.0",
    "@milkdown/transformer": "7.2.0",
    "@milkdown/theme-nord": "7.2.0",
    "@milkdown/plugin-history": "7.2.0",
    "@milkdown/plugin-upload": "7.2.0",
    "@milkdown/utils": "7.2.0",
    "@milkdown/plugin-listener": "7.2.0",
    "@type-config/strict": "^1.2.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react-dom": "0.0.0-experimental-ee8509801-20230117",
    "tslib": "^2.4.1",
    "@next-sdk/object-store-sdk": "^2.1.2",
    "@next-core/brick-http": "^2.8.1"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.5.1",
    "@next-core/test-next": "^1.0.5"
  }
}
