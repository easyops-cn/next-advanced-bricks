{
  "name": "@next-bricks/ui-test",
  "version": "1.1.21",
  "homepage": "https://github.com/easyops-cn/next-advanced-bricks/tree/master/bricks/ui-test",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-advanced-bricks.git",
    "directory": "bricks/ui-test"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types",
    "docs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./dist/manifest.json": "./dist/manifest.json",
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "build": "npm run build:main && npm run build:types",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json",
    "build:manifest": "cross-env NODE_ENV=production build-next-bricks --manifest-only",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@babel/parser": "^7.22.7",
    "@babel/standalone": "^7.22.9",
    "@babel/types": "^7.27.7",
    "@next-api-sdk/cmdb-sdk": "^1.1.1",
    "@next-api-sdk/next-builder-sdk": "^1.2.4",
    "@next-core/element": "^1.2.19",
    "@next-core/i18n": "^1.0.85",
    "@next-core/react-element": "^1.0.38",
    "@next-core/runtime": "^1.69.4",
    "@next-core/theme": "^1.6.1",
    "@next-shared/ui-test": "^1.0.28",
    "classnames": "^2.3.2",
    "file-saver": "^2.0.5",
    "idb-keyval": "^6.2.1",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "react": "0.0.0-experimental-ee8509801-20230117"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.23.14",
    "@next-core/test-next": "^2.0.1"
  },
  "peerDependencies": {
    "@next-bricks/icons": "*"
  }
}
