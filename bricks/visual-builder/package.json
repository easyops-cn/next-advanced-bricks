{
  "name": "@next-bricks/visual-builder",
  "version": "1.32.2",
  "homepage": "https://github.com/easyops-cn/next-bricks/tree/master/bricks/visual-builder",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-bricks.git",
    "directory": "bricks/visual-builder"
  },
  "license": "GPL-3.0",
  "type": "module",
  "sideEffects": true,
  "files": [
    "dist",
    "dist-types",
    "docs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dist/bricks.json": "./dist/bricks.json",
    "./dist/manifest.json": "./dist/manifest.json",
    "./*": {
      "types": "./dist-types/*/index.d.ts"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development build-next-bricks --watch",
    "build": "npm run build:main && npm run build:types",
    "build:main": "cross-env NODE_ENV=production build-next-bricks",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist-types --project tsconfig.json",
    "build:manifest": "cross-env NODE_ENV=production build-next-bricks --manifest-only",
    "test": "cross-env NODE_ENV='test' test-next",
    "test:ci": "cross-env NODE_ENV='test' CI=true test-next",
    "prepublishOnly": "cp package.json package.json.bak && npm pkg delete dependencies",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.5",
    "@formily/antd-v5": "^1.1.9",
    "@formily/core": "^2.3.1",
    "@formily/react": "^2.3.1",
    "@formily/reactive": "^2.3.1",
    "@next-api-sdk/next-builder-sdk": "^1.2.2",
    "@next-core/element": "^1.2.11",
    "@next-core/http": "^1.2.6",
    "@next-core/react-element": "^1.0.30",
    "@next-core/react-runtime": "^1.6.18",
    "@next-core/runtime": "^1.51.0",
    "@next-core/storyboard": "^0.6.14",
    "@next-core/theme": "^1.5.2",
    "@next-core/types": "^1.11.2",
    "@next-core/utils": "^1.7.16",
    "@next-shared/general": "^0.8.40",
    "@next-shared/markdown": "^0.1.5",
    "classnames": "^2.3.2",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "path-to-regexp": "^6.2.2",
    "react": "0.0.0-experimental-ee8509801-20230117"
  },
  "devDependencies": {
    "@next-core/build-next-bricks": "^1.22.4",
    "@next-core/test-next": "^1.1.5"
  },
  "peerDependencies": {
    "@next-bricks/basic": "*",
    "@next-bricks/form": "*",
    "@next-bricks/icons": "*",
    "@next-bricks/vs": "*"
  }
}
